<%@ page language="java" contentType="text/html; charset=EUC-KR"
    pageEncoding="EUC-KR"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>mywebsite</title>
</head>
<body>
<div class="wrapper">
	<form action="">
	<div class="wrap">
		<div class="subject">
			<span>회원가입</span>
		</div>
		<div class="id_wrap">
			<div class="id_name">아이디</div>
			<div class="id_input_box">
				<input class="id_input">
			</div>
		</div>
		<div class="pw_wrap">
			<div class="password">비밀번호</div>
			<div class="pw_input_box">
				<input class="pw_input">
			</div>
		</div>
		<div class="pwchk_wrap">
			<div class="pwCheck">비밀번호 확인</div>
			<div class="pwchk_input_box">
				<input class="pwchk_input">
			</div>
		</div>
		<div class="name_wrap">
			<div class="name">이름</div>
			<div class="name_input_box">
				<input class="name_input">
			</div>
		</div>
		<div class="mail_Wrap">
			<div class="mail">이메일</div>
			<div class="mail_input_box">
				<input class="mail_input">
			</div>
		</div>
		<div class="mail_check_wrap">
			<div class="mail_check_input_box">
				<input class="mail_check_input">
			</div>
			<div class="mail_check_button">
				<span>인증번호 전송</span>
			</div>
			<div class="clearfix"></div>
		</div>
		<div class="addr_wrap">
			<div class="addr_name">주소</div>
			<div class="addr_input_1_wrap">
				<div class="addr_input_1">
					<input class="addr_input_1">
				</div>
				<div class="addr_button">
					<span>주소 찾기</span>
				</div>
				<div class="clearfix"></div>
			</div>
			<div class="addr_input_2_wrap">
				<div class="addr_input_2_box">
					<div class="addr_input_2">
						<input class="addr_input_2">
					</div>
				</div>
			</div>
			<div class="addr_input_3_wrap">
				<div class="addr_input_3_box">
					<div class="addr_input_3">
						<input class="addr_input_3">
					</div>
				</div>
			</div>
			<div class="join_button_wrap">
				<input type="button" class="join_button" value="가입하기">
			</div>
		</div>
	</div>
	</form>
</div>
</body>
</html>